{
  "cli": {
    "version": ">= 13.4.2",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "extends": "production",
      "android": {
        "gradleCommand": ":app:assembleDebug",
        "buildType": "apk",
        "config": "android-build.yml"
      },
      "ios": { 
        "simulator": true,
        "autoIncrement": true,
        "withoutCredentials": true
      }
    },
    "preview": {
      "distribution": "internal",
      "developmentClient": true,
      "ios": {
        "simulator": true
      },
      "android": {
        "gradleCommand": ":app:assembleDebug",
        "buildType": "apk"
      }
    },
    "production": {
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}
